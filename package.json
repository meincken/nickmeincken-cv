{
  "name": "nickmeincken-cv",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.14",
    "@fortawesome/free-brands-svg-icons": "^5.7.1",
    "@fortawesome/free-solid-svg-icons": "^5.7.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-ga": "^2.5.7",
    "react-scripts": "3.2.0",
    "react-scroll": "^1.7.12",
    "react-waypoint": "^9.0.2",
    "styled-components": "^4.4.0"
  },
  "scripts": {
    "build:css": "postcss src/styles/main.css -o src/index.css",
    "watch:css": "postcss src/styles/main.css -o src/index.css -w",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "npm-run-all --parallel lint:*",
    "lint:stylelint": "stylelint 'src/css/*.css'",
    "lint:prettier": "prettier --check 'src/**/**/*.js'",
    "fix": "npm-run-all --parallel fix:*",
    "fix:stylelint": "stylelint --fix 'src/css/*.css'",
    "fix:prettier": "prettier --write 'src/**/**/*.{js,jsx,json}'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.4.8",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.1",
    "postcss-composes": "^0.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.18.2",
    "react-helmet": "^5.2.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-processor-styled-components": "^1.8.0"
  }
}
